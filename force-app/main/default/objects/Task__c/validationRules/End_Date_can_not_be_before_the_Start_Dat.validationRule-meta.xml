<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_can_not_be_before_the_Start_Dat</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(End_Date__c )), NOT(ISBLANK(Start_Date__c)),    End_Date__c  &lt; Start_Date__c)</errorConditionFormula>
    <errorMessage>End Date can not be before the Start Date</errorMessage>
</ValidationRule>
